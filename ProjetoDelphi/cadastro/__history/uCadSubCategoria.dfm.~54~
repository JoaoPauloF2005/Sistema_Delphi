  inherited QryListagem: TZQuery
    SQL.Strings = (
      'SELECT'
      '    sc.subCategoriaId,'
      '    sc.descricao,'
      '    sc.categoriaId,'
      '    c.descricao AS Descri'#231#227'oCategoria'
      'FROM '
      '    subCategorias sc'
      'LEFT JOIN '
      '    categorias c ON sc.categoriaId = c.categoriaId;')
